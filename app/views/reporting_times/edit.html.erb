<div id="content-header">
  <h1>Fee Invoice settings</h1>
  <div class='header-sep'>|</div>
  <div class='sub-header'>Edit</div>
  <div id="inner-tab-menu">
  </div>
</div>
<div id="page-yield">
  <div class="bread_crumb">
    <a href="/user/dashboard">Home</a> <div class="bread-crumb-separator"> &gt; </div> 
    <a href="/finance">Finance</a> <div class="bread-crumb-separator"> &gt; </div>
    <a href="/finance/fees_index">Fees</a> <div class="bread-crumb-separator"> &gt; </div>
    <a href="/bank_details">Fee Invoice settings</a> <div class="bread-crumb-separator"> &gt; </div> New
  </div>

  <div class="box">
    <div id="update_batch"></div>
    <div class="extender"></div>
  </div>
</div>
<div id="page-yield">
  <% unless flash[:notice].nil? %>
    <p class="flash-msg"> <%= flash[:notice] %> </p>
  <% end %>

  <% form_for(@reporting_time) do |f| %>
    <%= f.error_messages %>
    <div id="admission1_form">
      <div class="label-field-pair"><%#debugger%>
        <% if @reporting_time.time_in.present? %>
          <label for="student_grade">In TIme</label>
          <div class="text-input-bg">
            <%= f.text_field :time_in %>
          </div>
        <% else %>
          <label for="student_grade">Out TIme</label>
            <div class="text-input-bg">
              <%= f.text_field :out_time %>
            </div>
        <% end %>
      </div>
    </div>
    <%= f.submit"", :value => "► #{t('save_and_proceed')}", :class => "submit_button", :disable_with => "► #{t('please_wait')}" %>
  <% end %>
</div>


<%= link_to 'Show', @reporting_time %> |
