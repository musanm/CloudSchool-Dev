<div id="content-header">
  <%= show_header_icon %>
  <h1><%= t('reports_text') %></h1>
<div class='header-sep'>|</div>
<div class='sub-header'><%="#{t('batch')} #{t('fee_collection')}" %></div>
  
</div>


<div id="page-yield">
  <div class="bread_crumb">
    <%breadcrumb :report_batch_fee_collections,Batch.find(params[:id]) %>
    <%= render_breadcrumbs  %>
  </div>
<% unless flash[:notice].nil? %>
  <p class="flash-msg"> <%= flash[:notice] %> </p>
<% end %>
<div id="information">
  <%= render  :partial => 'batch_fees_collections'%>
</div>
</div>