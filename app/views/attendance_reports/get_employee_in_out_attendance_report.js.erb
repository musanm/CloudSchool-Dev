<% if @error.present? %>
  j("#employeee_register").html("")
  j("#error").html("<%= @error %>")  
<% else %>
  j("#employeee_register").html('<%=escape_javascript(render :partial => "attendance_reports/employee_in_out_report", :department => @department, :date => @date) %>');
  j(".pdf_report_div").removeClass("none")
  j(".pdf_report_div").html('<%=escape_javascript(link_to "Pdf Report", employee_in_out_attendance_pdf_report_attendance_reports_path(:advance_search=> params["advance_search"]),  :class => "pdf_report_btn",:target => "_blank") %>');
<% end %>
